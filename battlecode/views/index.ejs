  	<link href="styles.css" rel="stylesheet" type="text/css">
    <link rel='stylesheet' type="text/css" href='./codeMirrorSimple/codemirror.css'>

		<h1>BattleCode</h1>
		<div class = 'prompt'>
			<%- prompt %>
		</div>
		<div class='user-handles'>
			<div class='handle' id='player0'></div>
			<div class='handle' id='player1'></div>
		</div>

		</div class='code-container'>
			<textarea rows='20' class='play0' id='play0Code'>
				<%- code %>
			</textarea>
			<textarea rows='20' class='play1' id='play1Code'>
				<%- code %>
			</textarea><br>
		<div class = 'btn-container'>
			<button class='kick1'>Kick Player 1<br>(2 remaining)</button>
		</div>
		<div class = 'btn-container'>
			<button class='kick0'>Kick Player 0<br>(2 remaining)</button>
		</div>

		<div class='btn-container'>
			<button class='submit0'>Submit Code Player 0!</button>
		</div>
		<div class = 'btn-container'>
			<button class='submit1'>Submit Code Player 1!</button>
		</div>
		<div class='play0result'>Output for Player 0</div>
		<div class='play1result'>Output for Player 1</div>

		<div class='playName'><strong>You are spectating</strong></div>


		<script src='./lib/jquery/dist/jquery.min.js'></script>
    <script src='./lib/lodash/dist/lodash.min.js'></script>
    <script src='./lib/socket.io-client/socket.io.js'></script>
    <script src='./codeMirrorSimple/codemirror.js'></script>
    <script src='./codeMirrorSimple/javascript.js'></script>
		<script src='./app.js'></script>

	</body>
</html>